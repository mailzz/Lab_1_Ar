@model WebApplication2.Models.Directions
@{
    ViewBag.Title = "Добавление предметна";
}
<form asp-action="Create" asp-controller="Directions">
    <div class="form-group">
        <label asp-for="group" class="control-label">Название</label>
        <input type="text" asp-for="group" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="nubmer" class="control-label">Количество часов</label>
        <input type="text" asp-for="nubmer" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Отправить" class="btn btn-default" />
    </div>
</form>
